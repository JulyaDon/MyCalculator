<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>

<VBox alignment="center" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
       <TextField fx:id="TextFieldResult" prefHeight="25.0" prefWidth="140.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></TextField>
       <HBox spacing="10.0">
          <children>
              <Button fx:id="buttonAC" onAction="#onACclick" prefHeight="25.0" prefWidth="60.0" text="AC" />
              <Button fx:id="buttonDelete" onAction="#onDeletec" prefHeight="25.0" prefWidth="60.0" text="Delete" />
          </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
   
       </HBox>
   
       <HBox spacing="10.0">
      <children>
              <Button fx:id="button7" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="7" />
              <Button fx:id="button8" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="8" />
              <Button fx:id="button9" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="9" />
              <Button fx:id="buttonMulti" onAction="#onMulticlick" prefHeight="25.0" prefWidth="25.0" text="*" />
      </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
       </HBox>
   
       <HBox spacing="10.0">
      <children>
              <Button fx:id="button4" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="4" />
              <Button fx:id="button5" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="5" />
              <Button fx:id="button6" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="6" />
              <Button fx:id="buttonDel" onAction="#onDelclick" prefHeight="25.0" prefWidth="25.0" text="/" />
      </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
       </HBox>
   
       <HBox spacing="10.0">
      <children>
              <Button fx:id="button1" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="1" />
              <Button fx:id="button2" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="2" />
              <Button fx:id="button3" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="3">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
              <Button fx:id="buttonPlus" onAction="#onPlusclick" text="+" />
      </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
       </HBox>
   
       <HBox spacing="10.0">
      <children>
              <Button fx:id="button0" onAction="#onNclick" prefHeight="25.0" prefWidth="25.0" text="0" />
              <Button fx:id="buttonComa" onAction="#onComaclick" prefHeight="25.0" prefWidth="25.0" text="," />
              <Button fx:id="buttonEqual" onAction="#onEqualclick" prefHeight="25.0" prefWidth="25.0" text="=" />
              <Button fx:id="buttonMinus" onAction="#onMinusiclick" prefHeight="25.0" prefWidth="25.0" text="-" />
      </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
       </HBox>
   </children>
</VBox>
